    "koa2-cors": "^2.0.5",
    "moment": "^2.19.3",
    "node-env-file": "^0.1.8",
    "normalize.css": "^7.0.0",
    "octicons": "^7.1.0",
    "qiniu": "^7.1.1",
    "querystring": "^0.2.0",
    "vue": "^2.5.13",
    "vue-i18n": "^7.4.0",
    "vue-iscroll-view": "^1.0.3",
